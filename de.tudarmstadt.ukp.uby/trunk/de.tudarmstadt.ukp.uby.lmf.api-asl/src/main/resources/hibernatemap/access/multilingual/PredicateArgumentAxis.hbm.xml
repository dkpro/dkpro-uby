<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>  
<class name="de.tudarmstadt.ukp.lmf.model.multilingual.PredicateArgumentAxis" table="PredicateArgumentAxis" node="PredicateArgumentAxis" dynamic-insert="true">

    <id name="id" column="predicateArgumentAxisId" node="@id" type="string"/>
    
     <property name="axisType" column="axisType" node="@axisType">    
	   	<type name="de.tudarmstadt.ukp.lmf.hibernate.EnumUserType">
	       	<param name="enumClassName">de.tudarmstadt.ukp.lmf.model.enums.EPredicateArgumentAxisType</param>         	
	   	</type>
	</property>
    
    <many-to-one name="semanticPredicateOne" class="de.tudarmstadt.ukp.lmf.model.semantics.SemanticPredicate" 
		 column="semanticPredicateOne" node="@semanticPredicateOne" foreign-key="none" not-found="ignore" index="semanticPredicateOne_IDX"/>
    	
   	<many-to-one name="semanticPredicateTwo" class="de.tudarmstadt.ukp.lmf.model.semantics.SemanticPredicate" 
		 column="semanticPredicateTwo" node="@semanticPredicateTwo" foreign-key="none" not-found="ignore" index="semanticPredicateTwo_IDX"/>
    
   	<many-to-one name="semanticArgumentOne" class="de.tudarmstadt.ukp.lmf.model.semantics.SemanticArgument" 
		 column="semanticArgumentOne" node="@semanticArgumentOne" foreign-key="none" not-found="ignore" index="semanticArgumentOne_IDX"/>
   
   	<many-to-one name="semanticArgumentTwo" class="de.tudarmstadt.ukp.lmf.model.semantics.SemanticArgument" 
		 column="semanticArgumentTwo" node="@semanticArgumentTwo" foreign-key="none" not-found="ignore" index="semanticArgumentTwo_IDX"/>

 	<many-to-one name="lexiconOne" class="de.tudarmstadt.ukp.lmf.model.core.Lexicon" 
		 column="lexiconOne" node="@lexiconOne" foreign-key="none" not-found="ignore" index="lexiconOne_IDX"/>

 	<many-to-one name="lexiconTwo" class="de.tudarmstadt.ukp.lmf.model.core.Lexicon" 
		 column="lexiconTwo" node="@lexiconTwo" foreign-key="none" not-found="ignore" index="lexiconTwo_IDX"/>
	
    <property name="confidence" column="confidence" node="@confidence" type="double" />    
	
	<many-to-one name="metaData" class="de.tudarmstadt.ukp.lmf.model.meta.MetaData" column="metaDataId"
    	 node="@metaData" foreign-key="none" not-found="ignore" index="metaDataId_IDX"/>
    	 
 </class>
</hibernate-mapping>